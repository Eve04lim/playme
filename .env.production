# .env.production
# 本番環境用の環境変数

# API設定
VITE_API_BASE_URL=https://api.playme.app/api
VITE_APP_ENV=production

# Spotify API設定（本番用）
VITE_SPOTIFY_CLIENT_ID=production_spotify_client_id
VITE_SPOTIFY_REDIRECT_URI=https://playme.app/auth/spotify/callback

# Apple Music API設定（本番用）
VITE_APPLE_MUSIC_DEVELOPER_TOKEN=production_apple_music_developer_token

# その他の設定
VITE_APP_NAME=Playme
VITE_APP_VERSION=1.0.0

# ログレベル
VITE_LOG_LEVEL=error

# モック設定
VITE_ENABLE_MOCKS=false